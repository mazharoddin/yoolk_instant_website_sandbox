<h1 class="page-title">services</h1>
{% for category in listing.service_categories %}
  {% for service in category.services %}
    <div class="catalog">
      <a href="/services/kh12806-office-rental-services/59-ground-floor">
        <h2 class="catalog-title">{{ service.name }}</h2>
      </a>
      <div class="catalog-image">
        {% if service.photos.first != blank %}
          <a href="#" class="cboxElement">
            {{ service.photos.first.attachment_meta.large.url | asset_path | image_tag }}
          </a>
        {% endif %}
      </div>
      <ol class="catalog-thumbnails">
        {% if service.photos.first != blank %}
          <li class="catalog-thumbnails-image" data-photo-url="#">
            <a class="active" href="" rel="" title="" >
              {{ service.photos.first.attachment_meta.thumbnail.url | asset_path | image_tag }}
            </a>
          </li>
        {% endif %}
      </ol>
      <div class="catalog-info">
        <ul class="catalog-info-specifications">
          {% for property_hash in service.properties %}
            {% for property in property_hash %}
              <li class="clearfix product-list">
                <span class="ellipsis">{{ property[0] }}</span><span class="ellipsis">{{ property[1] }}</span>
              </li>
            {% endfor %}
          {% endfor %}
        </ul>
      </div>
    </div>
  {% endfor %}
{% endfor %}